{
    "name": "stock-predictor",
    "version": "1.0.0",
    "private": true,
    "description": "Stock predictor by Lecosoft",
    "license": "MIT",
    "author": "Lecosoft",
    "main": "electron.js",
    "scripts": {
        "build": "vite build --base=/stock-predictor",
        "clean": "rimraf dist public *.tgz",
        "predeploy": "npm run build && touch dist/.nojekyll && cp dist/index.html dist/404.html",
        "deploy": "gh-pages --dotfiles -d dist",
        "prepare": "husky install",
        "start": "NODE_ENV=development vite",
        "test": "jest",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "upgrade-dependencies": "npx npm-upgrade -pd",
        "electron": "electron .",
        "electron-dev": "concurrently \"npm start\" \"wait-on tcp:5173 && npm run electron\""
    },
    "lint-staged": {
        "package.json": "sort-package-json",
        "*.md": "prettier --write",
        "*.{ts,tsx,js}": [
            "prettier --write"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false,
        "trailingComma": "es5",
        "printWidth": 120,
        "arrowParens": "avoid",
        "endOfLine": "auto",
        "parser": "typescript"
    },
    "dependencies": {
        "axios": "^1.12.2",
        "bootstrap": "^5.3.8",
        "date-fns": "^4.1.0",
        "plotly.js": "^3.1.0",
        "react": "^19.1.1",
        "react-bootstrap": "^2.10.10",
        "react-dom": "^19.1.1",
        "react-plotly.js": "^2.6.0",
        "react-router-dom": "^7.9.1",
        "react-transition-group": "^4.4.5",
        "styled-components": "^6.1.19",
        "uuid": "^13.0.0",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@types/axios": "^0.14.4",
        "@types/plotly.js": "^3.0.6",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@types/react-plotly.js": "^2.6.3",
        "@types/react-transition-group": "^4.4.12",
        "@types/styled-components": "^5.1.34",
        "@types/uuid": "^11.0.0",
        "@vitejs/plugin-react": "^5.0.3",
        "concurrently": "^9.2.1",
        "electron": "^38.1.2",
        "electron-builder": "^26.0.12",
        "electron-is-dev": "^3.0.1",
        "gh-pages": "^6.3.0",
        "husky": "^9.1.7",
        "jest": "^30.1.3",
        "jest-environment-jsdom": "^30.1.2",
        "lint-staged": "^16.1.6",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "sort-package-json": "^3.4.0",
        "typescript": "^5.9.2",
        "vite": "^7.1.6",
        "wait-on": "^9.0.1"
    }
}
